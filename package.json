    "babel-compile": "^2.0.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-node5": "^11.0.2",
    "bluebird": "^3.4.5",
    "commander": "^2.9.0",
    "continuation-local-storage": "^3.1.4",
    "cookie": "^0.3.1",
    "debug": "^2.2.0",
    "eslint": "^3.4.0",
    "eslint-config-taskcluster": "^2.0.0",
